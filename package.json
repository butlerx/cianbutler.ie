{
  "name": "cianbutler.ie",
  "description": "personal site",
  "version": "2.0.1",
  "author": "Cian Butler <butlerx@notthe.cloud>",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "dev": "gatsby develop",
    "format": "prettier --write '{src,content}/**/*.{ts,tsx,md,scss}' '*.{js, json}'",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "type-check": "tsc --noEmit",
    "lint": "tslint --project .",
    "lint-sass": "sass-lint -vq src/**/*.scss"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix"
    ],
    "*.scss": [
      "prettier --write",
      "sass-lint -v"
    ],
    "*.{js,json,md}": [
      "prettier --write"
    ]
  },
  "license": "MIT",
  "keywords": [
    "gatsby"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/butlerx/cianbutler.ie"
  },
  "bugs": {
    "url": "https://github.com/butlerx/cianbutler.ie/issues"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "gatsby": "^2.21.24",
    "gatsby-image": "^2.4.3",
    "gatsby-plugin-manifest": "^2.4.2",
    "gatsby-plugin-offline": "^3.2.1",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sass": "^2.3.1",
    "gatsby-plugin-sharp": "^2.6.2",
    "gatsby-plugin-sitemap": "^2.4.2",
    "gatsby-plugin-tslint": "^0.0.2",
    "gatsby-plugin-typescript": "^2.4.2",
    "gatsby-source-filesystem": "^2.3.1",
    "gatsby-source-github-api": "^0.2.1",
    "gatsby-transformer-remark": "^2.8.7",
    "gatsby-transformer-sharp": "^2.5.2",
    "gatsby-transformer-yaml": "^2.4.1",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-markdown": "^4.3.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "netlify-cli": "^2.51.0",
    "prettier": "^2.0.5",
    "sass-lint": "^1.13.1",
    "tslint": "^6.1.2",
    "tslint-config-airbnb-base": "^0.3.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
