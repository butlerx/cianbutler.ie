{
  "name": "cianbutler.ie",
  "description": "personal site",
  "version": "2.0.0",
  "author": "Cian Butler <butlerx@notthe.cloud>",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write '{src,content}/**/*.{ts,tsx,md,scss}' '*.{js, json}'",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "type-check": "tsc --noEmit",
    "lint": "tslint --project .",
    "lint-sass": "sass-lint -vq src/**/*.scss"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "sass-lint -v",
      "git add"
    ],
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "MIT",
  "keywords": [
    "gatsby"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/butlerx/cv"
  },
  "bugs": {
    "url": "https://github.com/butlerx/cv/issues"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-brands-svg-icons": "^5.8.2",
    "@fortawesome/free-regular-svg-icons": "^5.8.2",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "gatsby": "^2.5.5",
    "gatsby-image": "^2.1.0",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.1",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.37",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-plugin-tslint": "^0.0.2",
    "gatsby-plugin-typescript": "^2.0.14",
    "gatsby-source-filesystem": "^2.0.36",
    "gatsby-source-github-api": "^0.1.0",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.19",
    "gatsby-transformer-yaml": "^2.1.12",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.0.8"
  },
  "devDependencies": {
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "husky": ">=1",
    "lint-staged": ">=8",
    "prettier": "^1.17.1",
    "sass-lint": "^1.13.1",
    "tslint": "^5.16.0",
    "tslint-config-airbnb-base": "^0.3.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
